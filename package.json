{
  "name": "vue-i18-context-menu",
  "displayName": "vue-i18-context-menu",
  "description": "",
  "version": "0.2.0",
  "publisher": "mathsgod",
  "author": {
    "name": "Raymond Chong",
    "email": "mathsgod@yahoo.com"
  },
  "engines": {
    "vscode": "^1.71.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:vue-i18-context-menu.t",
    "onCommand:vue-i18-context-menu.paste"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "menus": {
      "editor/context": [
        {
          "group": "1_modification",
          "command": "vue-i18-context-menu.t",
          "when": "editorTextFocus && resourceLangId == vue"
        },
        {
          "group": "1_modification",
          "command": "vue-i18-context-menu.paste",
          "when": "editorTextFocus"
        }
      ]
    },
    "commands": [
      {
        "command": "vue-i18-context-menu.t",
        "title": "Convert to i18n"
      },
      {
        "command": "vue-i18-context-menu.paste",
        "title": "Paste json i18n"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.71.0",
    "@types/glob": "^8.0.0",
    "@types/mocha": "^10.0.0",
    "@types/node": "16.x",
    "@typescript-eslint/eslint-plugin": "^5.38.1",
    "@typescript-eslint/parser": "^5.38.1",
    "eslint": "^8.24.0",
    "glob": "^8.0.3",
    "mocha": "^10.0.0",
    "typescript": "^4.8.4",
    "@vscode/test-electron": "^2.1.5"
  }
}
